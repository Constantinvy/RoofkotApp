CREATE TABLE UTILISATEUR (UTILISATEUR_ID INTEGER NOT NULL PRIMARY KEY AUTOINCREMENT, NOM TEXT NOT NULL, PRENOM TEXT NOT NULL, CATEGORIE TEXT NOT NULL, PHOTO BLOB)
CREATE TABLE JEUX (NOM TEXT NOT NULL UNIQUE PRIMARY KEY, DESCRIPTION TEXT NOT NULL, TEMPS INT NOT NULL, IMAGE BLOB)
CREATE TABLE PARTIE (PARTIE_ID INTEGER NOT NULL PRIMARY KEY AUTOINCREMENT, JEUX TEXT NOT NULL REFERENCES JEUX, DIFFICULTE TEXT NOT NULL, DATE TEXT NOT NULL)
CREATE TABLE REGLE (NOM TEXT NOT NULL UNIQUE PRIMARY KEY, DETAILS TEXT NOT NULL, DIFFICULTE TEXT NOT NULL)
CREATE TABLE JOUEUR (JOUEUR UTILISATEUR_ID NOT NULL REFERENCES UTILISATEUR, PARTIE PARTIE_ID NOT NULL REFERENCES PARTIE, REGLE REGLES NOT NULL REFERENCES REGLES)
